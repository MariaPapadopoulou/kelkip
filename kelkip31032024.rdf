    <skos:Concept rdf:about="#Seat_with_feet_for_one_person_with_back_without_arms">
        <skos:inScheme rdf:resource="http://www.ontologia.fr/OTB/kelkip31032024"/>
        <rdfs:label xml:lang="fr">chaise</rdfs:label>
        <skos:prefLabel xml:lang="fr">chaise</skos:prefLabel>
        <skos:definition xml:lang="fr">Siège avec pieds, pour une personne, avec dossier, sans bras.</skos:definition>
        <skos:note xml:lang="fr">Siège à dossier et généralement sans bras. Source : TLFi 31052017</skos:note>
        <skos:example xml:lang="fr">"Les cafés à l'entour avaient depuis longtemps couché sur leurs tables de marbre leurs chaises tendrement rabotées par des derrières peu soucieux de voyages. QUENEAU, Pierrot mon ami, 1942, p. 199." Source : TLFi 31052017</skos:example>
        <rdfs:label xml:lang="fr">chaise d'intérieur</rdfs:label>
        <skos:altLabel xml:lang="fr">chaise d'intérieur</skos:altLabel>
        <skos:definition xml:lang="fr">Siège avec dossier, pour une personne, avec pieds, sans bras.</skos:definition>
        <rdfs:label xml:lang="en">chair</rdfs:label>
