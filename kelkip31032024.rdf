    <skos:Concept rdf:about="#Seat_with_feet_for_several_people_with_arms_with_back">
        <skos:inScheme rdf:resource="http://www.ontologia.fr/OTB/kelkip31032024"/>
        <rdfs:label xml:lang="fr">canapé</rdfs:label>
        <skos:prefLabel xml:lang="fr">canapé</skos:prefLabel>
        <skos:definition xml:lang="fr">Siège pour plusieurs personnes, avec pieds, avec bras, avec dossier.</skos:definition>
        <skos:note xml:lang="fr">Siège à dossier, pourvu d'accoudoirs, où plusieurs personnes peuvent s'asseoir, pouvant aussi servir de lit de repos pour une personne. TLFi 02062017</skos:note>
        <skos:example xml:lang="fr">"Nous passâmes une heure ensemble, assis sur le même canapé, dans une grande pièce de la villa où il [Roosevelt] s'était installé. DE GAULLE, Mémoires de guerre, 1956, p. 79." TLFi 02062017</skos:example>
        <rdfs:label xml:lang="en">couch</rdfs:label>
        <skos:prefLabel xml:lang="en">couch</skos:prefLabel>
        <skos:definition xml:lang="en">Seat with feet, for several people, with arms, with back.</skos:definition>
        <skos:note xml:lang="en">"A couch is a long, comfortable seat for two or three people."
https:www.collinsdictionary.comdictionaryenglishcouch
2020 05 17</skos:note>
        <rdfs:label xml:lang="gr">καναπές</rdfs:label>
        <skos:prefLabel xml:lang="gr">καναπές</skos:prefLabel>
        <skos:definition xml:lang="gr">Κάθισμα για περισσότερα από ένα άτομα, με μπράτσα, πλάτη και πόδια. </skos:definition>
        <rdfs:label xml:lang="zh">长椅</rdfs:label>
        <skos:prefLabel xml:lang="zh">长椅</skos:prefLabel>
        <skos:broader rdf:resource="#Seat_with_feet_for_several_people"/>
        <foaf:depiction rdf:resource="http://ontologia.fr/Images/Seat/CanapéStyleLouisXV.PNG"/>
        <rdfs:seeAlso rdf:resource="https:fr.wikipedia.orgwikiCanap%C3%A9"/>
        <skos:exactMatch rdf:resource="http:vocab.getty.edupageaat300038636"/>
    </skos:Concept>

